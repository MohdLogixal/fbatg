<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE gsa-template SYSTEM "dynamosystemresource:/atg/dtds/gsa/gsa_1.0.dtd">
<gsa-template>

  <item-descriptor name="user">

    <!-- add the Facebook user id as a transient property -->
		<property name="facebookUserId" data-type="string" />

    <!-- add the authorization status as a transient property -->
    <property name="authStatus" data-type="enumerated">
			<attribute name="useCodeForValue" value="false"/>
			<option value="not_logged"/>
	    <option value="ATG_logged"/>
	    <option value="FB_logged"/>
	    <option value="FB_new_user"/>
	  </property>
            
  </item-descriptor>

</gsa-template>
